{
  "name": "slate2",
  "version": "0.1.0",
  "private": true,
  "description": "Creative generation pipeline for scroll-stopping assets with SSR gating.",
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "test:integration": "node tests/integration/m0.scraper.mjs && node tests/integration/m5.export.mjs",
    "typecheck": "pnpm -r run typecheck",
    "dead": "ts-prune -p tsconfig.ts-prune.json"
  },
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "zod": "3.23.8"
  }
}
